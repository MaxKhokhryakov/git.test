   "source": [
    "def own_zip(a, b):\n",
    "    if len(a) > len(b):\n",
    "        length = len(b)\n",
    "    else:\n",
    "        length = len(a)\n",
    "\n",
    "    for i in range(0, length):\n",
    "        d = (a[i], b[i])\n",
    "        yield d\n",
    "\n",
    "\n",
    "print(list(own_zip('MAI', 'Lambda')))\n",
    "print(list(own_zip('Hello', [5, 1, 0, 5])))"
   ]
