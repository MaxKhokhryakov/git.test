    "def own_enumerate(arr):\n",
    "    num = 0\n",
    "    for i in arr:\n",
    "        b = (num, i)\n",
    "        yield b\n",
    "        num += 1\n",
    "\n",
    "\n",
    "print(list(own_enumerate(['MAI', 'Lambda', 'Python'])))\n",
    "print(list(own_enumerate('Hello MAI!')))"
